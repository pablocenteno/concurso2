<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.concurso">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Concurso"
        tools:targetApi="31">
        <activity
            android:name=".recycler"
            android:exported="false"
            android:label="@string/title_activity_recycler"
            android:theme="@style/Theme.Concurso.NoActionBar">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DisplayMessageActivity"
            android:exported="false"></activity>
        <activity
            android:name=".DisplayMessageActivity2"
            android:exported="false"></activity>
        <activity
            android:name=".DisplayMessageActivity3"
            android:exported="false"></activity>
        <activity
            android:name=".DisplayMessageActivity4"
            android:exported="false"></activity>
    </application>

</manifest>